init
set workers to 0
set workers_upload to 0
set interval to 10
set serial to false
set uuid to '00000000-0000-0000-0000-000000000000'
set config_sync to false
open
# test: create storages
create cloud mock (type 'mock')
show clouds
{
  "mock": {
    "name": "mock",
    "type": "mock",
    "login": "",
    "password": "(secret)",
    "url": "",
    "debug": false
  }
}
create storage hot (sync false)
create storage cold (cloud 'mock', sync false)
# test: alter pipeline set partitions
alter pipeline set hot (partitions 2), cold
show pipeline
[{
  "name": "hot",
  "partitions": 2,
  "size": -1,
  "events": -1,
  "duration": -1
}, {
  "name": "cold",
